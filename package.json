{
    "name": "calumsieppert.me",
    "private": true,
    "description": "My personal website",
    "version": "0.1.0",
    "author": "Calum Sieppert",
    "scripts": {
        "build": "yarn clean && gatsby build",
        "develop": "yarn clean && gatsby develop",
        "develop-mobile": "yarn clean && gatsby develop -H 0.0.0.0 -p 8000",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "yarn develop",
        "serve": "yarn build && gatsby serve",
        "qserve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
        "get-schema": "get-graphql-schema http://localhost:8000/___graphql > schema.graphql"
    },
    "dependencies": {
        "@tailwindcss/typography": "^0.2.0",
        "esm": "^3.2.25",
        "gatsby": "^2.25.1",
        "gatsby-image": "^2.4.21",
        "gatsby-plugin-advanced-sitemap": "^1.5.6",
        "gatsby-plugin-catch-links": "^2.3.15",
        "gatsby-plugin-google-fonts": "^1.0.1",
        "gatsby-plugin-graphql-codegen": "^2.7.1",
        "gatsby-plugin-manifest": "^2.5.1",
        "gatsby-plugin-netlify": "^2.3.25",
        "gatsby-plugin-offline": "^3.3.1",
        "gatsby-plugin-postcss": "^3.0.4",
        "gatsby-plugin-react-helmet": "^3.3.14",
        "gatsby-plugin-react-svg": "^2.1.2",
        "gatsby-plugin-sharp": "^2.7.0",
        "gatsby-remark-autolink-headers": "^2.3.16",
        "gatsby-remark-check-links": "^2.1.0",
        "gatsby-remark-external-links": "^0.0.4",
        "gatsby-remark-images": "^3.3.40",
        "gatsby-remark-katex": "^3.3.14",
        "gatsby-remark-prismjs": "^3.5.16",
        "gatsby-remark-rewrite-relative-links": "^1.0.8",
        "gatsby-remark-smartypants": "^2.3.13",
        "gatsby-source-filesystem": "^2.4.0",
        "gatsby-transformer-remark": "^2.8.47",
        "gatsby-transformer-sharp": "^2.5.21",
        "graphql": "^14.0.2",
        "katex": "^0.12.0",
        "postcss-import": "^12.0.1",
        "prismjs": "^1.22.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "tailwindcss": "^1.9.6",
        "ts-node": "^9.0.0"
    },
    "devDependencies": {
        "gatsby-cli": "^2.8.22",
        "get-graphql-schema": "^2.1.2",
        "prettier": "2.1.2",
        "typescript": "^4.0.5"
    },
    "resolutions": {
        "graphql": "^14.0.2"
    },
    "license": "MIT"
}
